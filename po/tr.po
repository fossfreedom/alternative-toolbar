#: ../ui/altpreferences.ui.h:1
msgid "Top"
msgstr "En Üst"

#: ../ui/altpreferences.ui.h:2
msgid "Bottom"
msgstr "En Alt"

#: ../ui/altpreferences.ui.h:3
