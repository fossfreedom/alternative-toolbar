#: ../alternative-toolbar.py:235
msgid "Seek Backward"
msgstr "Siirry taaksepäin"

#: ../alternative-toolbar.py:238
msgid "Seek backward, in current track, by 5 seconds."
msgstr "Siirry taaksepäin 10 sekuntia nykyisessä kappaleessa."

#: ../alternative-toolbar.py:242
msgid "Seek Forward"
msgstr "Siirry eteenpäin"

#: ../alternative-toolbar.py:246
msgid "Seek forward, in current track, by 10 seconds."
msgstr "Siirry eteenpäin 10 sekuntia nykyisessä kappaleessa."

#: ../alternative-toolbar.py:258
msgid "Show Play-Controls Toolbar"
msgstr "Toistosäädinten työkalupalkki"

#: ../alternative-toolbar.py:262
